{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/market-data.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/market-stream.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/candles.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/market-data",
      "destination": "/api/market-data.js"
    },
    {
      "source": "/api/market/data",
      "destination": "/api/market-data.js"
    },
    {
      "source": "/api/market-stream",
      "destination": "/api/market-stream.js"
    },
    {
      "source": "/api/market/candles",
      "destination": "/api/candles.js"
    },
    {
      "source": "/api/user/apikeys",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/user/api-keys",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
