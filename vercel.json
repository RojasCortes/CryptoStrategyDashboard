{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/market-data",
      "dest": "/api/market-data.js"
    },
    {
      "src": "/api/market-stream",
      "dest": "/api/market-stream.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/((?!api).*)",
      "dest": "/index.html"
    }
  ]
}
